{
  "name": "gxclient",
  "version": "3.0.0",
  "description": "A client to interact with gxchain via http",
  "main": "./dist/index.js",
  "bin": {
    "gxclient": "./bin/gxclient"
  },
  "scripts": {
    "clean": "rm -rf ./dist/* ./build/*",
    "prebuild": "npm run clean",
    "postbuild": "npm run browserify && npm run build",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build tsconfig.json -w",
    "webpack": "webpack",
    "prepublish": "npm run build",
    "browserify": "browserify --full-paths dist/index.js --standalone gxclient -o build/gxclient.js -d -t ",
    "jsdoc": "vuepress-jsdoc --source=./lib/src --dist=./docs --folder=api --readme=./docs/copy/API.md && npm run copy:doc",
    "dev:doc": "vuepress dev docs",
    "build:doc": "vuepress build docs",
    "deploy:doc": "gh-pages -d docs-dist",
    "copy:doc": "./docs/bin/copy.sh",
    "analysis": "browserify-breakdown build/gxclient.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxchain/gxclient-node.git"
  },
  "keywords": [
    "gxchain",
    "client",
    "exchange-api",
    "gxs",
    "gxc"
  ],
  "author": "David Lan <lanhaoxiang@qq.com> (https://github.com/lanhaoxiang)",
  "contributors": [
    "David Lan <lanhaoxiang@qq.com> (https://github.com/lanhaoxiang/)"
  ],
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gxchain/gxchain-client-node/issues"
  },
  "homepage": "https://github.com/gxchain/gxchain-client-node#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "browserify": "^16.5.1",
    "browserify-breakdown": "^1.0.4",
    "chai": "^4.2.0",
    "gh-pages": "^2.0.1",
    "mocha": "^5.2.0",
    "rn-nodeify": "github:tradle/rn-nodeify",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "uglify-js": "^3.4.6",
    "unminified-webpack-plugin": "^2.0.0",
    "vuepress": "^1.0.0",
    "vuepress-jsdoc": "^1.7.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1"
  },
  "dependencies": {
    "typescript": "^4.0.5",
    "axios": "^0.19.0",
    "chalk": "^2.4.1",
    "commander": "^2.17.1",
    "create-hash": "^1.2.0",
    "debug": "^3.1.0",
    "events": "^1.1.1",
    "gxbjs": "^1.5.8",
    "lodash": "^4.17.19",
    "prompts": "^2.3.2",
    "randombytes": "^2.1.0"
  }
}
